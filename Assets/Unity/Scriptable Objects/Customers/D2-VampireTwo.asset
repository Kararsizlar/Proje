    - sentence: ...
      personTalking: 
      personClip: {fileID: 0}
      personSprite: 0
      customSprite: {fileID: 0}
  customerDialogueAtEndSuccess:
    sentenceData:
    - sentence: Her zaman ki gibi harika. Bu bana iyi geliyor.
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Kulland\u0131\u011F\u0131n kemik tozu g\xFC\xE7lenmemi sa\u011Fl\u0131yor."
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "\u0130\u015Fimizde en iyisi olmaya \xE7al\u0131\u015F\u0131yoruz."
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Buray\u0131 neden kurdun?"
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Asl\u0131nda ben b\xFCy\xFCc\xFC de\u011Filim."
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Anlam\u0131\u015Ft\u0131m. B\xFCy\xFCc\xFC olmak i\xE7in fazla iyimsersin."
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Kar\u0131m b\xFCy\xFCc\xFCyd\xFC. Yak\u0131n zamanda \xF6ld\xFC.
        Bende buray\u0131 \nkurdum."
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
