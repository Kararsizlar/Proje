  customerDialogueAtStart:
    sentenceData:
    - sentence: "Gale d\xFCkkan\u0131n\u0131 a\xE7m\u0131\u015Ft\u0131. Kap\u0131
        sesi duyulur."
      personTalking: 
      personClip: {fileID: 0}
      personSprite: 0
      customSprite: {fileID: 0}
    - sentence: 'M-merhabaa '
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Ho\u015F geldiniz. Sorununuz nedir?"
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Ben bir vampirim. \xC7ok susad\u0131m. Dostum kalmad\u0131. \n\u0130nsanlar
        onlar\u0131n kan\u0131n\u0131 i\xE7erim diye benden korkuyorlar."
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 4
      customSprite: {fileID: 0}
    - sentence: "Korkmakta hakl\u0131lar galiba."
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "S-san\u0131r\u0131m. Bana vejetaryen bir kan iksiri yapabilir misin?"
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: Hemeeen geliyor!
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 2
      customSprite: {fileID: 0}
  customerDialogueAtEndFail:
    sentenceData:
    - sentence: "Bu i\u011Fren\xE7 bunda insan kan\u0131 m\u0131 var! Bir daha buraya
        gelmice\u011Fim"
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 3
      customSprite: {fileID: 0}
    - sentence: ...
      personTalking: 
      personClip: {fileID: 0}
      personSprite: 0
      customSprite: {fileID: 0}
  customerDialogueAtEndSuccess:
    sentenceData:
    - sentence: "Harika olmu\u015F! San\u0131r\u0131m ad\u0131m\u0131 s\xF6ylemeyi
        unuttum ben Edward."
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 2
      customSprite: {fileID: 0}
    - sentence: "Tan\u0131\u015Ft\u0131\u011F\u0131m\u0131za memnun oldum."
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 2
      customSprite: {fileID: 0}
    - sentence: "Ne zamand\u0131r buradas\u0131n?"
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 2
      customSprite: {fileID: 0}
    - sentence: "Asl\u0131nda bug\xFCn ilk g\xFCn\xFCm."
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Cad\u0131 olmak i\xE7in \xE7ok sevecen duruyorsun."
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Sende vampir olmak i\xE7in fazla empati yap\u0131yorsun. "
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Evet hakl\u0131s\u0131n."
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Nas\u0131l vampir oldun?"
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 3
      customSprite: {fileID: 0}
    - sentence: "Bir hastanede \xF6lmek \xFCzereymi\u015Fim. Doktorum bunu g\xF6ze
        \nalamam\u0131\u015F."
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 4
      customSprite: {fileID: 0}
    - sentence: "Neden vejetaryen bir vampir olmak istedi\u011Fini anlad\u0131m \ngaliba."
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Evet \xF6yle. \u0130ksir i\xE7in te\u015Fekk\xFCrler iyi ak\u015Famlar"
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "\u0130yi ak\u015Famlar!"
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
