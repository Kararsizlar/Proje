  customerDialogueAtStart:
    sentenceData:
    - sentence: "\u0130yi ak\u015Famlar!"
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 3
      customSprite: {fileID: 0}
    - sentence: ...
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 3
      customSprite: {fileID: 0}
    - sentence: "\u0130yi ak\u015Famlar ho\u015F geldiniz! Ne laz\u0131md\u0131?"
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Sakinle\u015Ftirme iksiri var m\u0131 elinizde malum yar\u0131n
        dolunay. "
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: 'Elinde delibal var diye duydum. '
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: Hemen geliyor.
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
  customerDialogueAtEndFail:
    sentenceData:
    - sentence: "Bu \u015Fey i\u011Fren\xE7! "
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 3
      customSprite: {fileID: 0}
  customerDialogueAtEndSuccess:
    sentenceData:
    - sentence: "Delibal\u0131n uyu\u015Fturucu etkisi oldu\u011Funu duymu\u015Ftum"
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 21300000, guid: d2af119ba56a53745ad2e31ff972485b, type: 3}
    - sentence: "Her \u015Feyin az\u0131 ve \xE7o\u011Fu zarard\u0131r. Az miktarda
        kullan\u0131nca iyi \nbir sakinle\u015Ftiricidir."
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Hakl\u0131s\u0131n ger\xE7ekten her \u015Feyin az\u0131 ve \xE7o\u011Fu
        zarar."
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 21300000, guid: 37f185c170dfa0e4787e0be52e67812d, type: 3}
    - sentence: "Bu olduk\xE7a iyi geldi. Te\u015Fekk\xFCr ederim..."
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 2
      customSprite: {fileID: 0}
    - sentence: Gale.
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 2
      customSprite: {fileID: 0}
    - sentence: "Te\u015Fekk\xFCrler Gale."
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Kurt adamlar\u0131n buna ihtiyac\u0131 oldu\u011Funu d\xFC\u015F\xFCnm\xFCyordum."
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 6
      customSprite: {fileID: 21300000, guid: e45375e72a62a404bb84f30d5b36834c, type: 3}
    - sentence: "Dolunayda kurta d\xF6n\xFC\u015Fmek ac\u0131 verici olabiliyor."
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Ger\xE7ekten k\xF6t\xFC kokuyorsun de\u011Fil mi Gale?"
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 6
      customSprite: {fileID: 21300000, guid: 1023fb9ed8509ac469b4e99d4c905f7e, type: 3}
    - sentence: "Asl\u0131nda o kadar k\xF6t\xFC koktu\u011Funu d\xFC\u015F\xFCnm\xFCyordum."
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 6
      customSprite: {fileID: 21300000, guid: d2af119ba56a53745ad2e31ff972485b, type: 3}
    - sentence: "Biz vampirler kurt adamlar\u0131n o kokusunu hemen al\u0131r\u0131z."
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 6
      customSprite: {fileID: 21300000, guid: d2af119ba56a53745ad2e31ff972485b, type: 3}
    - sentence: O kan iksiri mi?
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: Evet.
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 6
      customSprite: {fileID: 21300000, guid: bbca4af27bb63e241b2e1c14f920ad13, type: 3}
    - sentence: "\u0130kimizde t\xFCr\xFCm\xFCzden memnun de\u011Filiz galiba."
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 3
      customSprite: {fileID: 0}
    - sentence: "Bencil olmak zorunda de\u011Filiz."
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 6
      customSprite: {fileID: 21300000, guid: d2af119ba56a53745ad2e31ff972485b, type: 3}
    - sentence: Evet.
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 4
      customSprite: {fileID: 0}
    - sentence: "San\u0131r\u0131m ben gitsem iyi olucak g\xF6r\xFC\u015F\xFCr\xFCz
        Gale ve eee.."
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 6
      customSprite: {fileID: 21300000, guid: d2af119ba56a53745ad2e31ff972485b, type: 3}
    - sentence: "Kusura bakma ismi mi s\xF6ylemeyi unuttum. Bowyer."
      personTalking: X
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "\u0130yi ak\u015Famlar Bowyer. "
      personTalking: Edward
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 6
      customSprite: {fileID: 21300000, guid: 37f185c170dfa0e4787e0be52e67812d, type: 3}
    - sentence: "Buras\u0131 yeni a\xE7\u0131ld\u0131 galiba!"
      personTalking: Bowyer
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "Evet yeni a\xE7\u0131ld\u0131."
      personTalking: Gale
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: "\u0130ksir i\xE7in te\u015Fekk\xFCrler yar\u0131n i\xE7in beni idare
        eder san\u0131r\u0131m. \u0130yi ak\u015Famlar!"
      personTalking: Bowyer
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 1
      customSprite: {fileID: 0}
    - sentence: ...
      personTalking: 
      personClip: {fileID: 8300000, guid: fccaef4127e85d145b463b91b4fbd221, type: 3}
      personSprite: 0
      customSprite: {fileID: 0}
